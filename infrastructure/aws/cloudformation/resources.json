{"AWSTemplateFormatVersion": "2010-09-09",
 "Resources": {
   "InternetGatewaycreate": {
     "Type": "AWS::EC2::InternetGateway", 
     "Properties": {
       "Tags": [{"Key": "name", "Value": "create-csye6225-InternetGateway"}]
     }
   }
  ,"VPCcreate": {
     "Type": "AWS::EC2::VPC", 
